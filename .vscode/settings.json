{
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"C_Cpp.intelliSenseEngine": "Tag Parser",
	"files.associations": {
		"*.h": "c"
	},
	"editor.formatOnSave": true,
	"[c]": {
		"editor.autoIndent": false
	},
	"clang-format.executable": "clang-format",
	"clang-format.language.c.enable": true,
	"clang-format.language.c.style": "{BasedOnStyle: Google, IndentWidth: 4, IndentCaseLabels: true, TabWidth: 4, UseTab: ForIndentation, ColumnLimit: 0}",
	"clang.cflags": [
		"-std=c11",
		"-ffreestanding",
		"-Wall",
		"-Wextra",
		"-Werror",
		"-masm=intel",
		"-g",
		"-O0",
		"-I ../../../include",
		"-funsigned-char"
	]
}